label Type
x-rha-select-loading-indicator(loading='typesLoading')
  ui-select(
    theme='bootstrap',
    id='rha-case-search_types',
    ng-disabled='!securityService.loginStatus.isLoggedIn',
    ng-model='CaseService.type',
    ng-change='onchange()')
    ui-select-match(placeholder='Select a Type') {{$select.selected.name}}
    ui-select-choices(
      repeat='type in CaseService.types | filter: $select.search',
      style='overflow: auto;')
      div {{type.name}}
